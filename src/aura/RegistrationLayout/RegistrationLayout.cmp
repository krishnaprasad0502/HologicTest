<aura:component controller="CommunityNavigationController" implements="forceCommunity:availableForAllPageTypes,forceCommunity:layout,forceCommunity:themeLayout" access="global" >
	
    <!-- Attribute Declaration -->
    <aura:attribute name="search" type="Aura.Component[]"/>
	<aura:attribute name="sidebarFooter" type="Aura.Component[]"/>
    <aura:attribute name="SupportHeader" type="Aura.Component[]"/>
    <aura:attribute name="SupportFooter" type="Aura.Component[]"/>
    
    <div class="slds-grid slds-wrap .slds-grid_pull-padded-xxx-small logoContainer" style="background: lightgray;">
    	<div class="patient-card slds-col--padded--medium slds-size--1-of-1 slds-medium-size--1-of-1 slds-large-size--1-of-1 slds-p-bottom--small">
            {!v.SupportHeader}
  		</div>
    </div>    
    <div class="slds-grid slds-wrap .slds-grid_pull-padded-xxx-small" style="padding-top: 50px;background:#f5f5f5">
        <div class="patient-card slds-col--padded--medium slds-size--1-of-1 slds-medium-size--1-of-1 slds-large-size--2-of-12 slds-p-bottom--small">
			&nbsp;
        </div>    
        <div class="patient-card slds-col--padded--medium slds-size--1-of-1 slds-medium-size--1-of-1 slds-large-size--8-of-12 slds-p-bottom--small">
            {!v.body}
        </div>
        <div class="patient-card slds-col--padded--medium slds-size--1-of-1 slds-medium-size--1-of-1 slds-large-size--2-of-12 slds-p-bottom--small">
			&nbsp;
        </div>
    </div> 
    <div class="slds-grid slds-wrap .slds-grid_pull-padded-xxx-small logoContainer" style="background: lightgray;">
    	<div class="patient-card slds-col--padded--medium slds-size--1-of-1 slds-medium-size--1-of-1 slds-large-size--1-of-1 slds-p-bottom--small">
            {!v.SupportFooter}
  		</div>
    </div>
</aura:component>