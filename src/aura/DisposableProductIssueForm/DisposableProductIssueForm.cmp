                                <br/>
                                <ui:outputText aura:id="nameOutput" class ="slds-text-heading_Small"/>
                                <lightning:textarea label="Provide a detail description of the exposure" class="TextAreaStyle slds-text-color_weak" aria-describedby="error-02" name="input2" value="{!v.q2Answer}" />
                                <div class="slds-form-element__help TextErrorMessage" id="error-02">{!$Label.c.Communtiy_Case_Creation_Q1_Error}</div>
                            </div> 
                            <aura:set Attribute="else">
                                <div>
                                    <br/>
                                    <ui:outputText aura:id="nameOutput" class ="slds-text-heading_Small" />
                                    <lightning:textarea label="Provide a detail description of the exposure" class="TextAreaStyle slds-text-color_weak" name="input2" value="{!v.q2Answer}" />
                                </div> 
                            </aura:set>
                        </aura:if>
                    </aura:if> 
                </div>
                <!-- Compliance Question 2 End-->
                <!-- Compliance Question 3 start-->
                <div class="slds-col slds-size_1-of-1 slds-medium-size_1-of-1 slds-large-size_1-of-1 leftAlign rightAlign" style="margin-top:25px">
                    <div class="slds-grid slds-text-heading_Small">
                        <abbr class="slds-required" title="required">*</abbr>
                        <b>{!$Label.c.Community_Case_Comment_Q4}</b><br/>
                    </div>
