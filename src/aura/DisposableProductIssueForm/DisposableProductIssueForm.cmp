    <aura:attribute name="q2Answer" type="string"/>
    <aura:attribute name="displayQ2TextBox" type="boolean" default="false"/>
    
    <aura:attribute name="q3Options" type="List" default="[
    	{'label': 'Yes', 'value': 'Yes'},
    	{'label': 'No', 'value': 'No'}
    ]"/>
