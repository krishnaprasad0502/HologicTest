                                <br/>
                                <lightning:textarea label="Provide a detail description of the injury" class="TextAreaStyle slds-text-color_weak" aria-describedby="error-01" name="input3" value="{!v.q1Answer}" />
                                <div class="slds-form-element__help TextErrorMessage" id="error-01">{!$Label.c.Communtiy_Case_Creation_Q1_Error}</div>
                            </div> 
                            <aura:set Attribute="else">
                                <div>
                                    <br/>
                                    <lightning:textarea label="Provide a detail description of the injury" class="TextAreaStyle slds-text-color_weak" name="input3" value="{!v.q1Answer}" />
                                </div> 
                            </aura:set>
                        </aura:if>
                    </aura:if>  
                </div>
                <!-- Compliance Question 1 End-->
                <!-- Compliance Question 2 Start-->
                <div class="slds-col slds-size_1-of-1 slds-medium-size_1-of-1 slds-large-size_1-of-1 leftAlign rightAlign" style="margin-top:25px">
