                <div class="slds-form-element">
                    <div class="slds-form-element__control">
                        <aura:if isTrue="{!v.showS1NumberError}">
                            <div style="margin-top: -10px;" class ="slds-form-element slds-has-error ">
                                <br/>
                                <lightning:input required ="true" aura:id="Validation5" disabled="false" name="input5"  aria-describedby="error-SN2" value="{!v.SerialNumber}" label="Serial Number"/>
                                <!--<div class="slds-form-element__help TextErrorMessage" id="error-SN2">Required Field</div>-->
                            </div> 
                            <aura:set Attribute="else">
                                <div>
                                    <br/>
                                    <lightning:input required ="true" aura:id="Validation5" disabled="false" name="input5" value="{!v.SerialNumber}" label="Serial Number"/>
                                </div> 
                            </aura:set>
                        </aura:if>
                        
                        
                        
                        <!--<lightning:input name="input5" disabled="false" value="{!v.SerialNumber}" label="Serial Number"/>-->
                    </div>
				</div>
                <div class="slds-form-element">
                    <div class="slds-form-element__control">
                        <aura:if isTrue="{!v.showS2Error}">
                            <div style="margin-top: -10px;" class ="slds-form-element slds-has-error ">
                                <br/>
                                <lightning:input aura:id="Validation2" name="validation" messageWhenValueMissing="Required Field" required ="true" aria-describedby="error-S1" disabled="false" value="{!v.NumberOfPatient}" label="Number of Patients Involved"/>
                                <div class="slds-form-element__help TextErrorMessage" id="error-S1">Required Field</div>
                            </div> 
                            <aura:set Attribute="else">
                                <div>
                                    <br/>
                                    <lightning:input aura:id="Validation2" name="validation" messageWhenValueMissing="Required Field" required ="true" disabled="false" value="{!v.NumberOfPatient}" label="Number of Patients Involved"/>
                                </div> 
                            </aura:set>
                        </aura:if>
                    </div>
				</div>                
            </div>
            <div class="slds-col slds-size_1-of-1 slds-medium-size_1-of-1 slds-large-size_1-of-1  leftAlign rightAlign">
                <div class="slds-form-element">
                    <div class="slds-form-element__control">
                        <aura:if isTrue="{!v.showS3Error}">
                            <div style="margin-top: -10px;" class ="slds-form-element slds-has-error ">
                                <br/>
                                <lightning:textarea aura:id="Validation3" name="validation" messageWhenValueMissing="Required Field" required ="true" aria-describedby="error-S1" disabled="false" value="{!v.DetailedDescription}" label="{!$Label.c.Community_Disposable_Text_Q1}"/>
                                <div class="slds-form-element__help TextErrorMessage" id="error-S1">Required Field</div>
                            </div> 
                            <aura:set Attribute="else">
                                <div>
                                    <br/>
                                    <lightning:textarea aura:id="Validation3" name="validation" messageWhenValueMissing="Required Field" required ="true" disabled="false" value="{!v.DetailedDescription}" label="{!$Label.c.Community_Disposable_Text_Q1}"/>
                                </div> 
                            </aura:set>
                        </aura:if>
                    </div>
                </div>           
            </div>
            <div class="slds-col slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-2  leftAlign rightAlign">
                <div class="slds-form-element">
                    <div class="slds-form-element__control">
                        <lightning:input name="input5" disabled="false" value="{!v.SalesRep}" label="{!$Label.c.Community_Disposable_Text_Q3}"/>
                    </div>
				</div>
            </div>
            <div class="slds-col slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-2  leftAlign rightAlign">
