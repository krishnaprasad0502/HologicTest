                                        <b>{!$Label.c.Community_Case_Comment_Q6}</b><br/>
                                    </div>
                                    <div class="slds-grid">
                                        <lightning:radioGroup name="radioGroup5" options="{!v.q5Options}" value="{! v.q5Value }" 
                                                              type="radio" variant="label-hidden" class="customRadioCls" 
                                                              onchange=""/>
