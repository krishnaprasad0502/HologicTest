                        <abbr class="slds-required" title="required">*</abbr>
                        <b>{!$Label.c.Community_Case_Comment_Q3}</b><br/>
                    </div>
                    <aura:if isTrue="{!v.q2Required}">
                        <div class="slds-grid slds-has-error">
