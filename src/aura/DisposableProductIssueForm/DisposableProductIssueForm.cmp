    <aura:attribute name="q3Answer" type="string"/>
    <aura:attribute name="displayQ3TextBox" type="boolean" default="false"/>
    
    <aura:attribute name="q4Options" type="List" default="[
    	{'label': 'Yes', 'value': 'Yes'},
    	{'label': 'No', 'value': 'No'}
    ]"/>
    <aura:attribute name="q4Value" type="string"/>
    <aura:attribute name="q4Answer" type="string"/>
    <aura:attribute name="displayQ4TextBox" type="boolean" default="false"/>
    
    <aura:attribute name="q7Options" type="List" default="[
    	{'label': 'Yes', 'value': 'Yes'},
    	{'label': 'No', 'value': 'No'}
    ]"/>
    <aura:attribute name="q7Value" type="string"/>
    <aura:attribute name="q7Answer" type="string"/>
    <aura:attribute name="displayQ7TextBox" type="boolean" default="false"/>
    
    <aura:attribute name="q5Options" type="List" default="[
